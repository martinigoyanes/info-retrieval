{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "java",
			"name": "Launch KGramIndex",
			"request": "launch",
			"mainClass": "ir.KGramIndex",
			"args": [
				"-f",
				"kgram_test.txt",
				"-p",
				"patterns.txt",
				"-k",
				"2",
				"-kg",
				"ve"
			]
		},
		{
			"type": "java",
			"name": "Launch Engine",
			"request": "launch",
			"mainClass": "${file}",
			"args": [
				"-d",
				"../data/davisWiki",
				"-l",
				"ir22.png",
				"-p",
				"patterns.txt",
				"-ni"
			]
		},
		{
			"type": "python",
			"name": "Current Python File",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"python.pythonPath": "/Library/Frameworks/Python.framework/Versions/3.8/bin/python3"
		}
	]
}